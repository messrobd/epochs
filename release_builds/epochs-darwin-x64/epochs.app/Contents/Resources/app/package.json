{
  "name": "epochs",
  "productName": "epochs",
  "version": "0.1.0",
  "description": "visualiser for epochs ",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon='assets/icons/mac/32421-birthday-cake-icon-emoji-png-1.png.png.icns' --prune=true --out=release_builds --app-bundle-id='messrobd.epochs' --osx-sign",
    "package-win": "electron-packager . epochs --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/32421-birthday-cake-icon-emoji-png-1.png.png.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Epochs\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/messrobd/timespans.git"
  },
  "author": "rob messenger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/messrobd/timespans/issues"
  },
  "homepage": "https://github.com/messrobd/timespans#readme",
  "devDependencies": {
    "electron": "^2.0.9",
    "electron-packager": "^12.1.2"
  },
  "dependencies": {
    "electron-osx-sign": "^0.4.10",
    "jquery": "^3.3.1"
  }
}
